    int REGNO;
    float MARKS;
}s1,s2,s3;
int main()
{
    strcpy(s1.NAME,"P SOMA SHANKAR REDDY");
    s1.REGNO=192465066;
    s1.MARKS=85.99;
    
    strcpy(s2.NAME,"N VINSENT");
    s2.REGNO=192425202;
    s2.MARKS=99.99;
    
    strcpy(s3.NAME,"A THRI LOKESH");
    s3.REGNO=192456321;
    s3.MARKS=98.99;

  printf("STUDENT NAME : %s\n",s1.NAME);
  printf("REGISTRATION NO : %d\n",s1.REGNO) ; 
