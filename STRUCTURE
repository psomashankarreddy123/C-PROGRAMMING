  printf("REGISTRATION NO : %d\n",s2.REGNO) ; 
  printf("MARKS : %.2f\n",s2.MARKS);
  
  printf("STUDENT NAME : %s\n",s3.NAME);
  printf("REGISTRATION NO : %d\n",s3.REGNO) ; 
  printf("MARKS : %.2f\n",s3.MARKS);
  
return 0;
}


